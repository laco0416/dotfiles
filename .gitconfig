[core]
	excludesfile = /Users/laco/.gitignore_global
	autocrlf = input
	editor = nano
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[user]
	email = suguru.inatomi@gmail.com
	name = Suguru Inatomi
[color]
	ui = auto
[url "git@github.com:"]
    # insteadOf = https://github.com/
[commit]
	template = /Users/laco/.stCommitMsg
